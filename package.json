{
  "name": "@hit/ui-kit",
  "version": "1.0.4",
  "description": "UI Kit contract for HIT feature packs - interfaces and context only",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "lucide-react": "^0.460.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "hit",
    "ui-kit",
    "headless",
    "feature-pack"
  ],
  "license": "MIT"
}
