{
  "name": "@hit/ui-kit",
  "version": "1.0.128",
  "description": "HIT UI kit",
  "main": "src/index.tsx",
  "types": "src/index.tsx",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "default": "./src/index.tsx"
    },
    "./components/*": {
      "types": "./src/components/*",
      "default": "./src/components/*"
    },
    "./hooks/*": {
      "types": "./src/hooks/*",
      "default": "./src/hooks/*"
    },
    "./theme": {
      "types": "./src/theme/index.ts",
      "default": "./src/theme/index.ts"
    },
    "./tokens/*": {
      "types": "./src/tokens/*",
      "default": "./src/tokens/*"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@tanstack/react-table": "^8.0.0",
    "react": "19.2.3"
  },
  "dependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tanstack/react-table": "^8.0.0",
    "lucide-react": "^0.460.0",
    "typescript": "^5"
  },
  "license": "MIT"
}
